
fn println(msg: String): Unit => print(msg + "\n")
fn printerrln(msg: String): Unit => printerr(msg + "\n")

fn print(x: String): Unit => {
  java {
    "System.out.print(x);"
  }
  ()
}

fn printf(x: String, ...): Unit => {
  java {
    "System.out.printf(x, var_args);"
  }
  ()
}

fn printerr(x: String): Unit => {
  java {
    "System.err.print(x);"
  }
  ()
}

fn format(x: String, ...): String => {
  val str = "()"
  java {
    "str = String.format(x, var_args);"
  }
  str
}


